{
    "name": "apothan/open-tour-website-lib",
    "type": "library",
    "description": "CMS for a travel website",
    "keywords": ["travel","tour","CMS"],
    "homepage": "https://clicct.com",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Adam Pothan",
            "email": "adam@clicct.com",
            "homepage": "http://clicct.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.3.*",
        "symfony/twig-bundle": "5.3.*",
        "symfony/yaml": "5.3.*",
        "sensio/framework-extra-bundle": "^6.1",
        "doctrine/annotations": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "Apothan\\OpenTourLibBundle\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
